{
  "connection": "default",
  "collectionName": "categories",
  "info": {
    "name": "category",
    "description": "special categories related to groups"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "groups": {
      "collection": "group",
      "dominant": true,
      "via": "categories"
    }
  }
}